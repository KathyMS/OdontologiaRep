{% include 'AdministradorBundle:Default:administrador_header.html.twig' %}
{% include 'AdministradorBundle:Default:administrador_menu_horizontal.html.twig' %}
{% block body -%}
    <h1>Paciente</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Cedula</th>
                <td>{{ entity.cedula }}</td>
            </tr>
            <tr>
                <th>Fechadeingreso</th>
                <td>{{ entity.fechaDeIngreso|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Sexo</th>
                <td>{{ entity.sexo }}</td>
            </tr>
            <tr>
                <th>Edad</th>
                <td>{{ entity.edad }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Ocupacion</th>
                <td>{{ entity.ocupacion }}</td>
            </tr>
            <tr>
                <th>Carne</th>
                <td>{{ entity.carne }}</td>
            </tr>
            <tr>
                <th>Apartadopostal</th>
                <td>{{ entity.apartadoPostal }}</td>
            </tr>
            <tr>
                <th>Beca</th>
                <td>{{ entity.beca }}</td>
            </tr>
            <tr>
                <th>Carrera</th>
                <td>{{ entity.carrera }}</td>
            </tr>
            <tr>
                <th>Estadocivil</th>
                <td>{{ entity.estadoCivil }}</td>
            </tr>
            <tr>
                <th>Direccion</th>
                <td>{{ entity.direccion }}</td>
            </tr>
            <tr>
                <th>Telefonohabitacion</th>
                <td>{{ entity.telefonoHabitacion }}</td>
            </tr>
            <tr>
                <th>Telefonotrabajo</th>
                <td>{{ entity.telefonoTrabajo }}</td>
            </tr>
            <tr>
                <th>Telefonomovil</th>
                <td>{{ entity.telefonoMovil }}</td>
            </tr>
            <tr>
                <th>Correoelectronico</th>
                <td>{{ entity.correoElectronico }}</td>
            </tr>
            <tr>
                <th>Sintoma</th>
                <td>{{ entity.sintoma }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('paciente') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('paciente_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
  {% include 'AdministradorBundle:Default:administrador_footer.html.twig' %}